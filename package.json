{
  "name": "buzz-cli",
  "version": "4.1.0",
  "main": "index.js",
  "bin": {
    "buzz": "./bin/buzz.js"
  },
  "scripts": {
    "dev": "babel src --out-dir lib -w -D --ignore \"src/source/**/*\",\"src/internal/boilerplate\"",
    "build": "rm -rf lib && babel src --out-dir lib  -D --ignore \"src/source/**/*\",\"src/internal/boilerplate\" --minified && npm version patch"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env"
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime"
      ]
    ]
  },
