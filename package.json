    "@babel/preset-env": "^7.1.0",
    "@babel/runtime": "^7.1.2",
    "address": "^1.0.3",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "colors": "^1.3.2",
